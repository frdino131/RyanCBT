<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

<!-- CHANGING THE START TO WORK WITH THE CONTROLLER -->
<category>
    <pattern>SESSION7 *</pattern>
    <template>
        <!-- SET NAME BASED ON CONTROLLER INPUT -->
        <think><set name="name"><star/></set></think>
        Hello <get name = "name"/>! Welcome to session seven. This is already our last session together. The last four weeks have went by so fast. What's new with you since we last talked?
    </template>
</category>



</aiml>
